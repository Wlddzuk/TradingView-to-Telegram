{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/email_check.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/email_check",
      "dest": "api/email_check.py"
    },
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ]
}